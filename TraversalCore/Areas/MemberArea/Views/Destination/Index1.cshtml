@using Entity.Concrete
@model List<Destination>
@{
    ViewData["Title"] = "Index1";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

@await Html.PartialAsync("/Views/MemberLayoutPartial/_NavbarPartial.cshtml")
<section class="user-dashboard page-wrapper">
    <div class="container">
        <div class="row">
            <div class="col-md-12" style="position:relative;top:-150px;">
                <div class="dashboard-wrapper user-dashboard">
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Şehir</th>
                                    <th>Süre</th>
                                    <th>Kapasite</th>
                                    <th>Fiyat</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.DestinationID</td>
                                        <td>@item.City</td>
                                        <td>@item.DayNight</td>
                                        <td>@item.Capacity </td>
                                        <td>@item.Price₺ </td>
                                        <td><a href="" class="btn btn-default">Detaylar</a></td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>